-- [SUM MAX MIN] 최대값 구하기
SELECT MAX(DATETIME) "시간"
FROM ANIMAL_INS;

-- [SUM MAX MIN] 최소값 구하기
SELECT MIN(DATETIME) "시간"
FROM ANIMAL_INS;

-- [SUM MAX MIN] 동물 수 구하기
SELECT COUNT(*) COUNT
FROM ANIMAL_INS;

-- [SUM MAX MIN] 중복 제거하기
SELECT COUNT(DISTINCT NAME) COUNT
FROM ANIMAL_INS
WHERE NAME IS NOT NULL;